<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:theme="@style/penntheme"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="16dp"
    android:paddingBottom="16dp"
    tools:context="com.phsapp.phsapp.MainActivity"
    android:background="@drawable/backgroundnew">

    <ImageButton
        app:srcCompat="@drawable/academicsicon"
        android:id="@+id/AcademicsButton"
        android:scaleType="fitCenter"
        android:cropToPadding="false"
        android:adjustViewBounds="true"
        android:background="@null"
        android:layout_marginEnd="22dp"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginRight="22dp"
        android:layout_alignTop="@+id/PASButton"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:tag="academics"
        android:onClick="academAct"
        />

    <TextView android:id="@+id/AcademicsText"
        android:layout_height="wrap_content"
        android:text="Academics"
        android:ems="10"
        android:textColor="?attr/colorControlNormal"
        android:gravity="center_vertical|center_horizontal"
        android:layout_width="wrap_content"
        android:layout_alignBaseline="@+id/PASText"
        android:layout_alignBottom="@+id/PASText"
        android:layout_toRightOf="@+id/PASButton"
        android:layout_toEndOf="@+id/PASButton"
        android:paddingRight="10dp"
        android:textSize="18sp" />

    <ImageButton android:id="@+id/CounselingButton"
        app:srcCompat="@drawable/counselingicon"
        android:scaleType="fitCenter"
        android:cropToPadding="false"
        android:background="@null"
        android:layout_marginTop="44dp"
        android:adjustViewBounds="false"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_below="@+id/PASText"
        android:layout_alignLeft="@+id/NewsIcon"
        android:layout_alignStart="@+id/NewsIcon"
        android:layout_marginLeft="10dp"
        android:tag="counseling"
        android:onClick="counselingAct"/>

    <ImageButton android:id="@+id/ClubsButton"
        app:srcCompat="@drawable/clubs"
        android:scaleType="fitCenter"
        android:cropToPadding="false"
        android:adjustViewBounds="true"
        android:background="@null"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_above="@+id/ClubsText"
        android:layout_alignLeft="@+id/AcademicsButton"
        android:layout_alignStart="@+id/AcademicsButton"
        android:tag="clubs"
        android:onClick="clubAct"/>

    <TextView android:id="@+id/ClubsText"
        android:layout_height="wrap_content"
        android:text="     Clubs"
        android:ems="10"
        android:textColor="?attr/colorControlNormal"
        android:textSize="18sp"
        android:gravity="center_vertical|center_horizontal"
        android:paddingLeft="0dp"
        android:paddingStart="0dp"
        android:paddingEnd="0dp"
        android:layout_width="wrap_content"
        android:layout_alignBaseline="@+id/AthleticsText"
        android:layout_alignBottom="@+id/AthleticsText"
        android:layout_toRightOf="@+id/NewsText"
        android:layout_toEndOf="@+id/NewsText"
        android:layout_marginLeft="252dp"
        android:layout_marginStart="252dp" />

    <ImageButton android:id="@+id/CalendarButton"
        app:srcCompat="@drawable/calendar"
        android:scaleType="fitCenter"
        android:cropToPadding="false"
        android:adjustViewBounds="true"
        android:background="@null"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginLeft="36dp"
        android:layout_marginStart="36dp"
        android:layout_above="@+id/PASText"
        android:layout_toRightOf="@+id/NewsIcon"
        android:layout_toEndOf="@+id/NewsIcon"
        android:tag="calendar"
        android:onClick="calendarAct"/>

    <ImageButton android:id="@+id/NewsIcon"
        app:srcCompat="@drawable/newsicon"
        android:scaleType="fitCenter"
        android:cropToPadding="false"
        android:adjustViewBounds="true"
        android:background="@null"
        android:layout_marginLeft="26dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="50dp"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:tag="news"
        android:onClick="newsAct"
        />

    <TextView android:id="@+id/NewsText"
        android:layout_height="wrap_content"
        android:text="News"
        android:ems="10"
        android:layout_width="wrap_content"
        android:textColor="?attr/colorControlNormal"
        android:textSize="18sp"
        android:gravity="center_vertical|center_horizontal"
        android:layout_marginTop="105dp"
        android:layout_alignTop="@+id/NewsIcon"
        android:layout_toLeftOf="@+id/CalendarButton"
        android:layout_toStartOf="@+id/CalendarButton"
        android:layout_marginLeft="10dp" />

    <ImageButton android:id="@+id/PhotoGalleryButton"
        app:srcCompat="@drawable/photogallery"
        android:scaleType="fitCenter"
        android:cropToPadding="false"
        android:adjustViewBounds="true"
        android:background="@null"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignTop="@+id/ContactUsButton"
        android:layout_alignRight="@+id/CalendarButton"
        android:layout_alignEnd="@+id/CalendarButton"
        android:layout_marginRight="12dp"
        android:layout_marginEnd="12dp"
        android:tag="photogallery"
        android:onClick="photoAct"/>

    <TextView android:id="@+id/StaffText"
        android:layout_height="wrap_content"
        android:text="Staff"
        android:ems="10"
        android:textColor="?attr/colorControlNormal"
        android:textSize="18sp"
        android:gravity="center_vertical|center_horizontal"
        android:paddingLeft="0dp"
        android:layout_width="wrap_content"
        android:layout_alignBaseline="@+id/PhotoGalleryText"
        android:layout_alignBottom="@+id/PhotoGalleryText"
        android:layout_toLeftOf="@+id/PhotoGalleryButton"
        android:layout_toStartOf="@+id/PhotoGalleryButton" />

    <ImageButton android:id="@+id/PASButton"
        app:srcCompat="@drawable/pasicon"
        android:scaleType="fitCenter"
        android:cropToPadding="false"
        android:adjustViewBounds="true"
        android:background="@null"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_above="@+id/AcademicsText"
        android:layout_alignRight="@+id/ContactUsButton"
        android:layout_alignEnd="@+id/ContactUsButton"
        android:layout_marginRight="11dp"
        android:layout_marginEnd="11dp"
        android:tag="pas"
        android:onClick="pasAct"/>

    <TextView android:id="@+id/AthleticsText"
        android:layout_height="wrap_content"
        android:text="Athletics"
        android:ems="10"
        android:textColor="?attr/colorControlNormal"
        android:textSize="18sp"
        android:gravity="center_vertical|center_horizontal"
        android:paddingLeft="0dp"
        android:layout_width="wrap_content"
        android:layout_alignBaseline="@+id/FineArtsText"
        android:layout_alignBottom="@+id/FineArtsText"
        android:layout_alignLeft="@+id/CalendarButton"
        android:layout_alignStart="@+id/CalendarButton"
        android:layout_marginLeft="68dp"
        android:layout_marginStart="68dp" />

    <TextView android:id="@+id/CounselingText"
        android:layout_height="wrap_content"
        android:text="Counseling"
        android:ems="10"
        android:textColor="?attr/colorControlNormal"
        android:textSize="18sp"
        android:gravity="center_vertical|center_horizontal"
        android:paddingLeft="0dp"
        android:layout_width="wrap_content"
        android:layout_below="@+id/CounselingButton"
        android:layout_toLeftOf="@+id/ClubsText"
        android:layout_toStartOf="@+id/ClubsText" />

    <ImageButton android:id="@+id/AboutUsButton"
        app:srcCompat="@drawable/abouticon"
        android:scaleType="fitCenter"
        android:cropToPadding="false"
        android:adjustViewBounds="true"
        android:background="@null"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignTop="@+id/ContactUsButton"
        android:layout_alignLeft="@+id/ClubsButton"
        android:layout_alignStart="@+id/ClubsButton"
        android:tag="about"
        android:onClick="aboutAct"/>

    <TextView android:id="@+id/PhotoGallerytext"
        android:layout_height="wrap_content"
        android:text="About Penn"
        android:ems="10"
        android:textColor="?attr/colorControlNormal"
        android:textSize="18sp"
        android:gravity="center_vertical|center_horizontal"
        android:paddingLeft="0dp"
        android:layout_width="wrap_content"
        android:layout_alignBaseline="@+id/ContactUsText"
        android:layout_alignBottom="@+id/ContactUsText"
        android:layout_alignLeft="@+id/AcademicsText"
        android:layout_alignStart="@+id/AcademicsText" />

    <ImageButton android:id="@+id/ContactUsButton"
        app:srcCompat="@drawable/contacticon"
        android:scaleType="fitCenter"
        android:cropToPadding="false"
        android:adjustViewBounds="true"
        android:background="@null"
        android:layout_marginTop="59dp"
        android:layout_height="100dp"
        android:layout_width="100dp"
        android:layout_marginRight="25dp"
        android:layout_marginEnd="25dp"
        android:layout_below="@+id/ClubsText"
        android:layout_toLeftOf="@+id/AboutUsButton"
        android:layout_toStartOf="@+id/AboutUsButton"
        android:tag="contact"
        android:onClick="contactAct"/>

    <TextView android:id="@+id/FineArtsText"
        android:layout_height="wrap_content"
        android:text="Fine Arts"
        android:ems="10"
        android:textColor="?attr/colorControlNormal"
        android:textSize="18sp"
        android:gravity="center_vertical|center_horizontal"
        android:paddingLeft="0dp"
        android:layout_width="wrap_content"
        android:layout_alignBaseline="@+id/CounselingText"
        android:layout_alignBottom="@+id/CounselingText"
        android:layout_toLeftOf="@+id/ContactUsButton"
        android:layout_toStartOf="@+id/ContactUsButton" />

    <TextView android:id="@+id/CalendarText"
        android:layout_height="wrap_content"
        android:text="Calendar"
        android:ems="10"
        android:layout_width="wrap_content"
        android:textColor="?attr/colorControlNormal"
        android:textSize="18sp"
        android:gravity="center_vertical|center_horizontal"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_alignBaseline="@+id/NewsText"
        android:layout_alignBottom="@+id/NewsText"
        android:layout_alignLeft="@+id/FineArtsText"
        android:layout_alignStart="@+id/FineArtsText" />

    <TextView android:id="@+id/PhotoGalleryText"
        android:layout_height="wrap_content"
        android:text="Photo Gallery"
        android:ems="10"
        android:textColor="?attr/colorControlNormal"
        android:textSize="18sp"
        android:gravity="center_vertical|center_horizontal"
        android:paddingLeft="57dp"
        android:layout_width="wrap_content"
        android:layout_marginTop="88dp"
        android:layout_alignTop="@+id/PhotoGalleryButton"
        android:layout_toLeftOf="@+id/PASButton"
        android:layout_toStartOf="@+id/PASButton"
        android:layout_marginRight="11dp"
        android:layout_marginEnd="11dp" />

    <TextView android:id="@+id/PASText"
        android:layout_height="wrap_content"
        android:inputType="textMultiLine"
        android:ems="10"
        android:lines="2"
        android:text="Students and             Parents"
        android:textColor="?attr/colorControlNormal"
        android:textSize="18sp"
        android:layout_width="wrap_content"
        android:gravity="center_vertical|fill_vertical|center_horizontal"
        android:paddingTop="-8dp"
        android:layout_alignBaseline="@+id/CalendarText"
        android:layout_alignBottom="@+id/CalendarText"
        android:layout_alignLeft="@+id/AthleticsText"
        android:layout_alignStart="@+id/AthleticsText"
        android:layout_marginLeft="11dp"
        android:layout_marginStart="11dp" />

    <TextView android:id="@+id/ContactUsText"
        android:layout_height="wrap_content"
        android:text="Contact Us"
        android:ems="10"
        android:textColor="?attr/colorControlNormal"
        android:textSize="18sp"
        android:gravity="center_vertical|center_horizontal"
        android:paddingLeft="0dp"
        android:layout_width="wrap_content"
        android:layout_alignBaseline="@+id/PhotoGalleryText"
        android:layout_alignBottom="@+id/PhotoGalleryText"
        android:layout_alignLeft="@+id/PASText"
        android:layout_alignStart="@+id/PASText" />


</RelativeLayout>
